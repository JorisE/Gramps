prefix		= @prefix@
exec_prefix	= @exec_prefix@
bindir 		= @bindir@
datadir		= @datadir@/@PACKAGE@
INSTALL		= @INSTALL@
data		= ${datadir}
pycomp		= ${srcdir}/py-compile

@SET_MAKE@

all:

install:
	${INSTALL} -d ${data}
	${INSTALL} -c *.sxd table.* ${data}

uninstall:
	rm -f ${data}/*.sxd ${data}/table.html ${data}/table.sxc
	-rmdir ${data}

clean:
	-rm -f core *.pyo *.pyc *.bak *~	


